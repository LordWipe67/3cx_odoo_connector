<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="trecxcrm_tree_view">
        <field name="name">trecxcrm.tree</field>
        <field name="model">trecxcrm</field>
        <field name="arch" type="xml">
            <tree>
                <field name="operatore"/>
                <field name="telefono"/>
                <field name="name"/>
                <field name="cognome"/>
                <field name="data_inizio"/>
                <field name="data_fine"/>
                <field name="indirizzo_url" widget="url" readonly="1" text="Apri"/>
                <field name="note"/>
<!--                <field name="gestito"/>-->
                <field name="token_call"/>
            </tree>
        </field>
    </record>
</odoo>
